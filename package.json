  "description": "express-sharp-server is a middleware that implements a restful image server based on node-sharp",
  "main": "index.js",
  "files": [
    "index.js",
    "lib/"
  ],
  "author": "Marcel Bretschneider <info@3epnm.de>",
  "homepage": "https://github.com/3epnm/express-sharp-server",
  "license": "Apache-2.0",
  "dependencies": {
    "express": "^4.16.4",
    "js-yaml": ">=3.13.1",
    "json5": "^2.1.0",
    "jsonfile": "^5.0.0",
    "lodash": ">=4.17.12",
    "mathjs": "^5.3.1",
    "minimist": "^1.2.5",
    "mkdirp": "^0.5.1",
    "moment": "^2.29.2",
    "multer": "^1.4.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "sharp": "^0.21.0",
    "underscore": "^1.9.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.1.0",
    "sinon": "^7.2.2",
    "rimraf": "^2.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/3epnm/express-sharp-server"
  },
  "scripts": {
    "start": "SET NODE_ENV=dev && node test/server.js",
