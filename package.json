    "test-ci": "nyc mocha --exit tests/unit && nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "exclude": [
      "tests",
      "**/*.spec.js"
    ]
  },
  "keywords": [
    "jpeg",
    "png",
    "image",
    "resize",
    "polygon-extract",
